{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "System": "Warning",
            "Microsoft": "Warning"
        },
        "Console": {
            "LogLevel": {
                "Default": "Debug",
                "Compiler.SyntacticAnalysis.SyntaxAnalyser": "Warning",
                "Compiler.Ast.SymbolTable": "Warning",
                "Compiler.Compiler": "Warning",
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Warning"
            },
            "FormatterName": "Simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": false,
                "TimestampFormat": "HH:mm:ss ",
                "UseUtcTimestamp": true,
                "JsonWriterOptions": {
                    "Indented": true
                }
            }
        }
    }
}